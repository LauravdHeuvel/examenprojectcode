CREATE TABLE `seller` (
  `firstName` varchar(45) DEFAULT NULL,
  `lastName` varchar(45) DEFAULT NULL,
  `blocked` tinyint(1) DEFAULT NULL,
  `emailAddress` varchar(45) NOT NULL,
  PRIMARY KEY (`emailAddress`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

