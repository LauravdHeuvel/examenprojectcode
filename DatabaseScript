CREATE TABLE `passwords` (
  `password` varchar(45) DEFAULT NULL,
  `emailAddress` varchar(45) NOT NULL,
  PRIMARY KEY (`emailAddress`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

